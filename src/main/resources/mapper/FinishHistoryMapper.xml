<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="co.botechservices.novlnovl.infrastructure.mapper.FinishHistoryMapper">
    <select id="findFinishBooksByUser"
            resultType="co.botechservices.novlnovl.domain.book.dto.BookDto">
        select DISTINCT b.* from novl_finish_history f inner join novl_book b
        on f.book_id = b.id
        where f.user_id = #{userId}
    </select>
</mapper>